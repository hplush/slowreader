[
  {
    "name": "Files to download",
    "path": [
      "dist/**/*",
      "!dist/.well-known",
      "!dist/ui",
      "!dist/robots.txt",
      "!dist/{404,500}.html",
      "!dist/icon-*.{png,svg}",
      "!dist/favicon.ico",
      "!dist/assets/noto-sans-{cyrillic,greek,devanagari,vietnamese}-*.woff2",
      "!dist/assets/noto-sans-latin-ext-*.woff2"
    ],
    "limit": "85 KB"
  },
  {
    "name": "Core scripts to execute",
    "path": "dist/assets/index-*.js",
    "brotli": false,
    "limit": "135 KB"
  }
]
