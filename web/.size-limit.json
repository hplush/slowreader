[
  {
    "name": "Files to download",
    "path": [
      "dist/**/*",
      "!dist/.well-known",
      "!dist/ui",
      "!dist/robots.txt",
      "!dist/{404,500}.html",
      "!dist/icon-*.{png,svg}",
      "!dist/favicon.ico",
      "!dist/assets/devtools-*.js",
      "!dist/assets/*.woff",
      "!dist/assets/noto-*-{cyrillic,greek,devanagari,vietnamese}-*.woff2",
      "!dist/assets/noto-sans-mono-*.woff2",
      "!dist/assets/playpen-sans-*.woff2",
      "!dist/assets/*-{italic,math,latin-ext}-*.woff2"
    ],
    "limit": "180 KB"
  },
  {
    "name": "Core scripts to execute",
    "path": "dist/assets/index-*.js",
    "brotli": false,
    "limit": "270 KB"
  }
]
