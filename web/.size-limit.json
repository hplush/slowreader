[
  {
    "name": "Files to download",
    "path": [
      "dist/**/*",
      "!dist/.well-known",
      "!dist/ui/**",
      "!dist/404.html",
      "!dist/500.html",
      "!dist/error.avif",
      "!dist/assets/noto-sans-*.woff2",
      "!dist/assets/noto-sans-latin-wght-normal*.woff2"
    ],
    "limit": "55 KB"
  },
  {
    "name": "Core scripts to execute",
    "path": "dist/assets/index-*.js",
    "brotli": false,
    "limit": "135 KB"
  }
]
